[
  {
    "audioFile" : "Al-'Adiyat_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Al-'Adiyat_0_Translation",
    "transliterationKey" : "Al-'Adiyat_0_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_1",
    "text" : "وَالْعَادِيَاتِ ضَبْحًا",
    "translationKey" : "Al-'Adiyat_1_Translation",
    "transliterationKey" : "Al-'Adiyat_1_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_2",
    "text" : "فَالْمُورِيَاتِ قَدْحًا",
    "translationKey" : "Al-'Adiyat_2_Translation",
    "transliterationKey" : "Al-'Adiyat_2_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_3",
    "text" : "فَالْمُغِيرَاتِ صُبْحًا",
    "translationKey" : "Al-'Adiyat_3_Translation",
    "transliterationKey" : "Al-'Adiyat_3_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_4",
    "text" : "فَأَثَرْنَ بِهِ نَقْعًا",
    "translationKey" : "Al-'Adiyat_4_Translation",
    "transliterationKey" : "Al-'Adiyat_4_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_5",
    "text" : "فَوَسَطْنَ بِهِ جَمْعًا",
    "translationKey" : "Al-'Adiyat_5_Translation",
    "transliterationKey" : "Al-'Adiyat_5_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_6",
    "text" : "إِنَّ الْإِنْسَانَ لِرَبِّهِ لَكَنُودٌ",
    "translationKey" : "Al-'Adiyat_6_Translation",
    "transliterationKey" : "Al-'Adiyat_6_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_7",
    "text" : "وَإِنَّهُ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ",
    "translationKey" : "Al-'Adiyat_7_Translation",
    "transliterationKey" : "Al-'Adiyat_7_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_8",
    "text" : "وَإِنَّهُ لِحُبِّ الْخَيْرِ لَشَدِيدٌ",
    "translationKey" : "Al-'Adiyat_8_Translation",
    "transliterationKey" : "Al-'Adiyat_8_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_9",
    "text" : " أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِي الْقُبُورِ",
    "translationKey" : "Al-'Adiyat_9_Translation",
    "transliterationKey" : "Al-'Adiyat_9_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_10",
    "text" : "وَحُصِّلَ مَا فِي الصُّدُورِ",
    "translationKey" : "Al-'Adiyat_10_Translation",
    "transliterationKey" : "Al-'Adiyat_10_Transliteration"
  },
  {
    "audioFile" : "Al-'Adiyat_11",
    "text" : "إِنَّ رَبَّهُمْ بِهِمْ يَوْمَئِذٍ لَخَبِيرٌ",
    "translationKey" : "Al-'Adiyat_11_Translation",
    "transliterationKey" : "Al-'Adiyat_11_Transliteration"
  }
]
