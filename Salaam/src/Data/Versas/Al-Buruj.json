[
    {
        "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "translationKey" : "Al-Burooj_0_Translation",
        "transliterationKey" : "Al-Burooj_0_Transliteration",
        "audioFile" : "Al-Burooj_0"
    },
    {
        "text" : "وَالسَّمَاءِ ذَاتِ الْبُرُوجِ",
        "translationKey" : "Al-Burooj_1_Translation",
        "transliterationKey" : "Al-Burooj_1_Transliteration",
        "audioFile" : "Al-Burooj_1"
    },
    {
        "text" : "وَالْيَوْمِ الْمَوْعُودِ",
        "translationKey" : "Al-Burooj_2_Translation",
        "transliterationKey" : "Al-Burooj_2_Transliteration",
        "audioFile" : "Al-Burooj_2"
    },
    {
        "text" : "وَشَاهِدٍ وَمَشْهُودٍ",
        "translationKey" : "Al-Burooj_3_Translation",
        "transliterationKey" : "Al-Burooj_3_Transliteration",
        "audioFile" : "Al-Burooj_3"
    },
    {
        "text" : "قُتِلَ أَصْحَابُ الْأُخْدُودِ",
        "translationKey" : "Al-Burooj_4_Translation",
        "transliterationKey" : "Al-Burooj_4_Transliteration",
        "audioFile" : "Al-Burooj_4"
    },
    {
        "text" : "النَّارِ ذَاتِ الْوَقُودِ",
        "translationKey" : "Al-Burooj_5_Translation",
        "transliterationKey" : "Al-Burooj_5_Transliteration",
        "audioFile" : "Al-Burooj_5"
    },
    {
        "text" : "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
        "translationKey" : "Al-Burooj_6_Translation",
        "transliterationKey" : "Al-Burooj_6_Transliteration",
        "audioFile" : "Al-Burooj_6"
    },
    {
        "text" : "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُؤْمِنِينَ شُهُودٌ",
        "translationKey" : "Al-Burooj_7_Translation",
        "transliterationKey" : "Al-Burooj_7_Transliteration",
        "audioFile" : "Al-Burooj_7"
    },
    {
        "text" : "وَمَا نَقَمُوا مِنْهُمْ إِلَّا أَنْ يُؤْمِنُوا بِاللَّهِ الْعَزِيزِ الْحَمِيدِ",
        "translationKey" : "Al-Burooj_8_Translation",
        "transliterationKey" : "Al-Burooj_8_Transliteration",
        "audioFile" : "Al-Burooj_8"
    },
    
    {
        "text" : "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ ۚ وَاللَّهُ عَلَىٰ كُلِّ شَيْءٍ شَهِيدٌ",
        "translationKey" : "Al-Burooj_9_Translation",
        "transliterationKey" : "Al-Burooj_9_Transliteration",
        "audioFile" : "Al-Burooj_9"
    },
    {
        "text" : "إِنَّ الَّذِينَ فَتَنُوا الْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ ثُمَّ لَمْ يَتُوبُوا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيقِ",
        "translationKey" : "Al-Burooj_10_Translation",
        "transliterationKey" : "Al-Burooj_10_Transliteration",
        "audioFile" : "Al-Burooj_10"
    },
    {
        "text" : "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتٌ تَجْرِي مِنْ تَحْتِهَا الْأَنْهَارُ ۚ ذَٰلِكَ الْفَوْزُ الْكَبِيرُ",
        "translationKey" : "Al-Burooj_11_Translation",
        "transliterationKey" : "Al-Burooj_11_Transliteration",
        "audioFile" : "Al-Burooj_11"
    },
    {
        "text" : "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
        "translationKey" : "Al-Burooj_12_Translation",
        "transliterationKey" : "Al-Burooj_12_Transliteration",
        "audioFile" : "Al-Burooj_12"
    },
    {
        "text" : "إِنَّهُ هُوَ يُبْدِئُ وَيُعِيدُ",
        "translationKey" : "Al-Burooj_13_Translation",
        "transliterationKey" : "Al-Burooj_13_Transliteration",
        "audioFile" : "Al-Burooj_13"
    },
    {
        "text" : "وَهُوَ الْغَفُورُ الْوَدُودُ",
        "translationKey" : "Al-Burooj_14_Translation",
        "transliterationKey" : "Al-Burooj_14_Transliteration",
        "audioFile" : "Al-Burooj_14"
    },
    {
        "text" : "ذُو الْعَرْشِ الْمَجِيدُ",
        "translationKey" : "Al-Burooj_15_Translation",
        "transliterationKey" : "Al-Burooj_15_Transliteration",
        "audioFile" : "Al-Burooj_15"
    },
    {
        "text" : "فَعَّالٌ لِمَا يُرِيدُ ",
        "translationKey" : "Al-Burooj_16_Translation",
        "transliterationKey" : "Al-Burooj_16_Transliteration",
        "audioFile" : "Al-Burooj_16"
    },
    {
        "text" : "هَلْ أَتَاكَ حَدِيثُ الْجُنُودِ",
        "translationKey" : "Al-Burooj_17_Translation",
        "transliterationKey" : "Al-Burooj_17_Transliteration",
        "audioFile" : "Al-Burooj_17"
    },
    {
        "text" : "فِرْعَوْنَ وَثَمُودَ",
        "translationKey" : "Al-Burooj_18_Translation",
        "transliterationKey" : "Al-Burooj_18_Transliteration",
        "audioFile" : "Al-Burooj_18"
    },
    {
        "text" : "بَلِ الَّذِينَ كَفَرُوا فِي تَكْذِيبٍ",
        "translationKey" : "Al-Burooj_19_Translation",
        "transliterationKey" : "Al-Burooj_19_Transliteration",
        "audioFile" : "Al-Burooj_19"
    },
    {
        "text" : "وَاللَّهُ مِنْ وَرَائِهِمْ مُحِيطٌ",
        "translationKey" : "Al-Burooj_20_Translation",
        "transliterationKey" : "Al-Burooj_20_Transliteration",
        "audioFile" : "Al-Burooj_20"
    },
    {
        "text" : "بَلْ هُوَ قُرْآنٌ مَجِيدٌ",
        "translationKey" : "Al-Burooj_21_Translation",
        "transliterationKey" : "Al-Burooj_21_Transliteration",
        "audioFile" : "Al-Burooj_21"
    },
    {
        "text" : "فِي لَوْحٍ مَحْفُوظٍ",
        "translationKey" : "Al-Burooj_22_Translation",
        "transliterationKey" : "Al-Burooj_22_Transliteration",
        "audioFile" : "Al-Burooj_22"
    }
]
