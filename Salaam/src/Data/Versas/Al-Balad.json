[
  {
    "audioFile" : "Al-Balad_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Al-Balad_0_Translation",
    "transliterationKey" : "Al-Balad_0_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_1",
    "text" : "لَا أُقْسِمُ بِهَٰذَا الْبَلَدِ",
    "translationKey" : "Al-Balad_1_Translation",
    "transliterationKey" : "Al-Balad_1_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_2",
    "text" : "وَأَنْتَ حِلٌّ بِهَٰذَا الْبَلَدِ",
    "translationKey" : "Al-Balad_2_Translation",
    "transliterationKey" : "Al-Balad_2_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_3",
    "text" : "وَوَالِدٍ وَمَا وَلَدَ",
    "translationKey" : "Al-Balad_3_Translation",
    "transliterationKey" : "Al-Balad_3_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_4",
    "text" : "لَقَدْ خَلَقْنَا الْإِنْسَانَ فِي كَبَدٍ",
    "translationKey" : "Al-Balad_4_Translation",
    "transliterationKey" : "Al-Balad_4_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_5",
    "text" : "أَيَحْسَبُ أَنْ لَنْ يَقْدِرَ عَلَيْهِ أَحَدٌ",
    "translationKey" : "Al-Balad_5_Translation",
    "transliterationKey" : "Al-Balad_5_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_6",
    "text" : "يَقُولُ أَهْلَكْتُ مَالًا لُبَدًا",
    "translationKey" : "Al-Balad_6_Translation",
    "transliterationKey" : "Al-Balad_6_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_7",
    "text" : "أَيَحْسَبُ أَنْ لَمْ يَرَهُ أَحَدٌ",
    "translationKey" : "Al-Balad_7_Translation",
    "transliterationKey" : "Al-Balad_7_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_8",
    "text" : "أَلَمْ نَجْعَلْ لَهُ عَيْنَيْنِ",
    "translationKey" : "Al-Balad_8_Translation",
    "transliterationKey" : "Al-Balad_8_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_9",
    "text" : "وَلِسَانًا وَشَفَتَيْنِ",
    "translationKey" : "Al-Balad_9_Translation",
    "transliterationKey" : "Al-Balad_9_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_10",
    "text" : "وَهَدَيْنَاهُ النَّجْدَيْنِ",
    "translationKey" : "Al-Balad_10_Translation",
    "transliterationKey" : "Al-Balad_10_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_11",
    "text" : "فَلَا اقْتَحَمَ الْعَقَبَةَ",
    "translationKey" : "Al-Balad_11_Translation",
    "transliterationKey" : "Al-Balad_11_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_12",
    "text" : "وَمَا أَدْرَاكَ مَا الْعَقَبَةُ",
    "translationKey" : "Al-Balad_12_Translation",
    "transliterationKey" : "Al-Balad_12_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_13",
    "text" : "فَكُّ رَقَبَةٍ",
    "translationKey" : "Al-Balad_13_Translation",
    "transliterationKey" : "Al-Balad_13_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_14",
    "text" : "أَوْ إِطْعَامٌ فِي يَوْمٍ ذِي مَسْغَبَةٍ",
    "translationKey" : "Al-Balad_14_Translation",
    "transliterationKey" : "Al-Balad_14_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_15",
    "text" : "يَتِيمًا ذَا مَقْرَبَةٍ",
    "translationKey" : "Al-Balad_15_Translation",
    "transliterationKey" : "Al-Balad_15_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_16",
    "text" : "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ",
    "translationKey" : "Al-Balad_16_Translation",
    "transliterationKey" : "Al-Balad_16_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_17",
    "text" : "ثُمَّ كَانَ مِنَ الَّذِينَ آمَنُوا وَتَوَاصَوْا بِالصَّبْرِ وَتَوَاصَوْا بِالْمَرْحَمَةِ",
    "translationKey" : "Al-Balad_17_Translation",
    "transliterationKey" : "Al-Balad_17_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_18",
    "text" : "أُولَٰئِكَ أَصْحَابُ الْمَيْمَنَةِ",
    "translationKey" : "Al-Balad_18_Translation",
    "transliterationKey" : "Al-Balad_18_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_19",
    "text" : "وَالَّذِينَ كَفَرُوا بِآيَاتِنَا هُمْ أَصْحَابُ الْمَشْأَمَةِ",
    "translationKey" : "Al-Balad_19_Translation",
    "transliterationKey" : "Al-Balad_19_Transliteration"
  },
  {
    "audioFile" : "Al-Balad_20",
    "text" : "عَلَيْهِمْ نَارٌ مُؤْصَدَةٌ",
    "translationKey" : "Al-Balad_20_Translation",
    "transliterationKey" : "Al-Balad_20_Transliteration"
  }
]