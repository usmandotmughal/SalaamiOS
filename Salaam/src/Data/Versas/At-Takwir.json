[
    {
        "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "translationKey" : "At-Takwir_0_Translation",
        "transliterationKey" : "At-Takwir_0_Transliteration",
        "audioFile" : "At-Takwir_0"
    },
    {
        "text" : "إِذَا الشَّمْسُ كُوِّرَتْ",
        "translationKey" : "At-Takwir_1_Translation",
        "transliterationKey" : "At-Takwir_1_Transliteration",
        "audioFile" : "At-Takwir_1"
    },
    {
        "text" : "وَإِذَا النُّجُومُ انْكَدَرَتْ",
        "translationKey" : "At-Takwir_2_Translation",
        "transliterationKey" : "At-Takwir_2_Transliteration",
        "audioFile" : "At-Takwir_2"
    },
    {
        "text" : "وَإِذَا الْجِبَالُ سُيِّرَتْ",
        "translationKey" : "At-Takwir_3_Translation",
        "transliterationKey" : "At-Takwir_3_Transliteration",
        "audioFile" : "At-Takwir_3"
    },
    {
        "text" : "وَإِذَا الْعِشَارُ عُطِّلَتْ",
        "translationKey" : "At-Takwir_4_Translation",
        "transliterationKey" : "At-Takwir_4_Transliteration",
        "audioFile" : "At-Takwir_4"
    },
    {
        "text" : "وَإِذَا الْوُحُوشُ حُشِرَتْ",
        "translationKey" : "At-Takwir_5_Translation",
        "transliterationKey" : "At-Takwir_5_Transliteration",
        "audioFile" : "At-Takwir_5"
    },
    {
        "text" : "وَإِذَا الْبِحَارُ سُجِّرَتْ",
        "translationKey" : "At-Takwir_6_Translation",
        "transliterationKey" : "At-Takwir_6_Transliteration",
        "audioFile" : "At-Takwir_6"
    },
    {
        "text" : "وَإِذَا النُّفُوسُ زُوِّجَتْ",
        "translationKey" : "At-Takwir_7_Translation",
        "transliterationKey" : "At-Takwir_7_Transliteration",
        "audioFile" : "At-Takwir_7"
    },
    {
        "text" : "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ",
        "translationKey" : "At-Takwir_8_Translation",
        "transliterationKey" : "At-Takwir_8_Transliteration",
        "audioFile" : "At-Takwir_8"
    },
    
    {
        "text" : "بِأَيِّ ذَنْبٍ قُتِلَتْ",
        "translationKey" : "At-Takwir_9_Translation",
        "transliterationKey" : "At-Takwir_9_Transliteration",
        "audioFile" : "At-Takwir_9"
    },
    {
        "text" : "وَإِذَا الصُّحُفُ نُشِرَتْ",
        "translationKey" : "At-Takwir_10_Translation",
        "transliterationKey" : "At-Takwir_10_Transliteration",
        "audioFile" : "At-Takwir_10"
    },
    {
        "text" : "وَإِذَا السَّمَاءُ كُشِطَتْ",
        "translationKey" : "At-Takwir_11_Translation",
        "transliterationKey" : "At-Takwir_11_Transliteration",
        "audioFile" : "At-Takwir_11"
    },
    {
        "text" : "وَإِذَا الْجَحِيمُ سُعِّرَتْ",
        "translationKey" : "At-Takwir_12_Translation",
        "transliterationKey" : "At-Takwir_12_Transliteration",
        "audioFile" : "At-Takwir_12"
    },
    {
        "text" : "وَإِذَا الْجَنَّةُ أُزْلِفَتْ",
        "translationKey" : "At-Takwir_13_Translation",
        "transliterationKey" : "At-Takwir_13_Transliteration",
        "audioFile" : "At-Takwir_13"
    },
    {
        "text" : "عَلِمَتْ نَفْسٌ مَا أَحْضَرَتْ",
        "translationKey" : "At-Takwir_14_Translation",
        "transliterationKey" : "At-Takwir_14_Transliteration",
        "audioFile" : "At-Takwir_14"
    },
    {
        "text" : "فَلَا أُقْسِمُ بِالْخُنَّسِ",
        "translationKey" : "At-Takwir_15_Translation",
        "transliterationKey" : "At-Takwir_15_Transliteration",
        "audioFile" : "At-Takwir_15"
    },
    {
        "text" : "الْجَوَارِ الْكُنَّسِ",
        "translationKey" : "At-Takwir_16_Translation",
        "transliterationKey" : "At-Takwir_16_Transliteration",
        "audioFile" : "At-Takwir_16"
    },
    {
        "text" : "وَاللَّيْلِ إِذَا عَسْعَسَ",
        "translationKey" : "At-Takwir_17_Translation",
        "transliterationKey" : "At-Takwir_17_Transliteration",
        "audioFile" : "At-Takwir_17"
    },
    
    {
        "text" : "وَالصُّبْحِ إِذَا تَنَفَّسَ",
        "translationKey" : "At-Takwir_18_Translation",
        "transliterationKey" : "At-Takwir_18_Transliteration",
        "audioFile" : "At-Takwir_18"
    },
    {
        "text" : "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ",
        "translationKey" : "At-Takwir_19_Translation",
        "transliterationKey" : "At-Takwir_19_Transliteration",
        "audioFile" : "At-Takwir_19"
    },
    {
        "text" : "ذِي قُوَّةٍ عِنْدَ ذِي الْعَرْشِ مَكِينٍ",
        "translationKey" : "At-Takwir_20_Translation",
        "transliterationKey" : "At-Takwir_20_Transliteration",
        "audioFile" : "At-Takwir_20"
    },
    {
        "text" : "مُطَاعٍ ثَمَّ أَمِينٍ",
        "translationKey" : "At-Takwir_21_Translation",
        "transliterationKey" : "At-Takwir_21_Transliteration",
        "audioFile" : "At-Takwir_21"
    },
    {
        "text" : "وَمَا صَاحِبُكُمْ بِمَجْنُونٍ",
        "translationKey" : "At-Takwir_22_Translation",
        "transliterationKey" : "At-Takwir_22_Transliteration",
        "audioFile" : "At-Takwir_22"
    },
    {
        "text" : "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ",
        "translationKey" : "At-Takwir_23_Translation",
        "transliterationKey" : "At-Takwir_23_Transliteration",
        "audioFile" : "At-Takwir_23"
    },
    {
        "text" : "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ",
        "translationKey" : "At-Takwir_24_Translation",
        "transliterationKey" : "At-Takwir_24_Transliteration",
        "audioFile" : "At-Takwir_24"
    },
    {
        "text" : "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَجِيمٍ",
        "translationKey" : "At-Takwir_25_Translation",
        "transliterationKey" : "At-Takwir_25_Transliteration",
        "audioFile" : "At-Takwir_25"
    },
    {
        "text" : "فَأَيْنَ تَذْهَبُونَ",
        "translationKey" : "At-Takwir_26_Translation",
        "transliterationKey" : "At-Takwir_26_Transliteration",
        "audioFile" : "At-Takwir_26"
    },
    {
        "text" : "إِنْ هُوَ إِلَّا ذِكْرٌ لِلْعَالَمِينَ",
        "translationKey" : "At-Takwir_27_Translation",
        "transliterationKey" : "At-Takwir_27_Transliteration",
        "audioFile" : "At-Takwir_27"
    },
    {
        "text" : "لِمَنْ شَاءَ مِنْكُمْ أَنْ يَسْتَقِيمَ",
        "translationKey" : "At-Takwir_28_Translation",
        "transliterationKey" : "At-Takwir_28_Transliteration",
        "audioFile" : "At-Takwir_28"
    },
    {
        "text" : "وَمَا تَشَاءُونَ إِلَّا أَنْ يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ",
        "translationKey" : "At-Takwir_29_Translation",
        "transliterationKey" : "At-Takwir_29_Transliteration",
        "audioFile" : "At-Takwir_29"
    }
]
