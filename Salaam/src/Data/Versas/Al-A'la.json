[
  {
    "audioFile" : "Al-A'la_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Al-A'la_0_Translation",
    "transliterationKey" : "Al-A'la_0_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_1",
    "text" : "سَبِّحِ اسْمَ رَبِّكَ الْأَعْلَى",
    "translationKey" : "Al-A'la_1_Translation",
    "transliterationKey" : "Al-A'la_1_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_2",
    "text" : "الَّذِي خَلَقَ فَسَوَّىٰ",
    "translationKey" : "Al-A'la_2_Translation",
    "transliterationKey" : "Al-A'la_2_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_3",
    "text" : "وَالَّذِي قَدَّرَ فَهَدَىٰ",
    "translationKey" : "Al-A'la_3_Translation",
    "transliterationKey" : "Al-A'la_3_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_4",
    "text" : "وَالَّذِي أَخْرَجَ الْمَرْعَىٰ",
    "translationKey" : "Al-A'la_4_Translation",
    "transliterationKey" : "Al-A'la_4_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_5",
    "text" : "فَجَعَلَهُ غُثَاءً أَحْوَىٰ",
    "translationKey" : "Al-A'la_5_Translation",
    "transliterationKey" : "Al-A'la_5_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_6",
    "text" : "سَنُقْرِئُكَ فَلَا تَنْسَىٰ",
    "translationKey" : "Al-A'la_6_Translation",
    "transliterationKey" : "Al-A'la_6_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_7",
    "text" : "إِلَّا مَا شَاءَ اللَّهُ ۚ إِنَّهُ يَعْلَمُ الْجَهْرَ وَمَا يَخْفَىٰ",
    "translationKey" : "Al-A'la_7_Translation",
    "transliterationKey" : "Al-A'la_7_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_8",
    "text" : "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
    "translationKey" : "Al-A'la_8_Translation",
    "transliterationKey" : "Al-A'la_8_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_9",
    "text" : "فَذَكِّرْ إِنْ نَفَعَتِ الذِّكْرَىٰ",
    "translationKey" : "Al-A'la_9_Translation",
    "transliterationKey" : "Al-A'la_9_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_10",
    "text" : "سَيَذَّكَّرُ مَنْ يَخْشَىٰ",
    "translationKey" : "Al-A'la_10_Translation",
    "transliterationKey" : "Al-A'la_10_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_11",
    "text" : "وَيَتَجَنَّبُهَا الْأَشْقَى",
    "translationKey" : "Al-A'la_11_Translation",
    "transliterationKey" : "Al-A'la_11_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_12",
    "text" : "الَّذِي يَصْلَى النَّارَ الْكُبْرَىٰ",
    "translationKey" : "Al-A'la_12_Translation",
    "transliterationKey" : "Al-A'la_12_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_13",
    "text" : "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
    "translationKey" : "Al-A'la_13_Translation",
    "transliterationKey" : "Al-A'la_13_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_14",
    "text" : "قَدْ أَفْلَحَ مَنْ تَزَكَّىٰ",
    "translationKey" : "Al-A'la_14_Translation",
    "transliterationKey" : "Al-A'la_14_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_15",
    "text" : "وَذَكَرَ اسْمَ رَبِّهِ فَصَلَّىٰ",
    "translationKey" : "Al-A'la_15_Translation",
    "transliterationKey" : "Al-A'la_15_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_16",
    "text" : "بَلْ تُؤْثِرُونَ الْحَيَاةَ الدُّنْيَا",
    "translationKey" : "Al-A'la_16_Translation",
    "transliterationKey" : "Al-A'la_16_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_17",
    "text" : "وَالْآخِرَةُ خَيْرٌ وَأَبْقَىٰ",
    "translationKey" : "Al-A'la_17_Translation",
    "transliterationKey" : "Al-A'la_17_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_18",
    "text" : "إِنَّ هَٰذَا لَفِي الصُّحُفِ الْأُولَىٰ",
    "translationKey" : "Al-A'la_18_Translation",
    "transliterationKey" : "Al-A'la_18_Transliteration"
  },
  {
    "audioFile" : "Al-A'la_19",
    "text" : "صُحُفِ إِبْرَاهِيمَ وَمُوسَىٰ",
    "translationKey" : "Al-A'la_19_Translation",
    "transliterationKey" : "Al-A'la_19_Transliteration"
  }
]