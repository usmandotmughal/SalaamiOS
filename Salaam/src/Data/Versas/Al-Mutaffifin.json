[
    {
        "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "translationKey" : "Al-Mutaffifin_0_Translation",
        "transliterationKey" : "Al-Mutaffifin_0_Transliteration",
        "audioFile" : "Al-Mutaffifin_0"
    },
    {
        "text" : "وَيْلٌ لِلْمُطَفِّفِينَ",
        "translationKey" : "Al-Mutaffifin_1_Translation",
        "transliterationKey" : "Al-Mutaffifin_1_Transliteration",
        "audioFile" : "Al-Mutaffifin_1"
    },
    {
        "text" : "الَّذِينَ إِذَا اكْتَالُوا عَلَى النَّاسِ يَسْتَوْفُونَ",
        "translationKey" : "Al-Mutaffifin_2_Translation",
        "transliterationKey" : "Al-Mutaffifin_2_Transliteration",
        "audioFile" : "Al-Mutaffifin_2"
    },
    {
        "text" : "‎وَإِذَا كَالُوهُمْ أَوْ وَزَنُوهُمْ يُخْسِرُونَ",
        "translationKey" : "Al-Mutaffifin_3_Translation",
        "transliterationKey" : "Al-Mutaffifin_3_Transliteration",
        "audioFile" : "Al-Mutaffifin_3"
    },
    {
        "text" : "أَلَا يَظُنُّ أُولَٰئِكَ أَنَّهُمْ مَبْعُوثُونَ",
        "translationKey" : "Al-Mutaffifin_4_Translation",
        "transliterationKey" : "Al-Mutaffifin_4_Transliteration",
        "audioFile" : "Al-Mutaffifin_4"
    },
    {
        "text" : "‎لِيَوْمٍ عَظِيمٍ",
        "translationKey" : "Al-Mutaffifin_5_Translation",
        "transliterationKey" : "Al-Mutaffifin_5_Transliteration",
        "audioFile" : "Al-Mutaffifin_5"
    },
    {
        "text" : "يَوْمَ يَقُومُ النَّاسُ لِرَبِّ الْعَالَمِينَ",
        "translationKey" : "Al-Mutaffifin_6_Translation",
        "transliterationKey" : "Al-Mutaffifin_6_Transliteration",
        "audioFile" : "Al-Mutaffifin_6"
    },
    {
        "text" : "كَلَّا إِنَّ كِتَابَ الْفُجَّارِ لَفِي سِجِّينٍ",
        "translationKey" : "Al-Mutaffifin_7_Translation",
        "transliterationKey" : "Al-Mutaffifin_7_Transliteration",
        "audioFile" : "Al-Mutaffifin_7"
    },
    {
        "text" : "وَمَا أَدْرَاكَ مَا سِجِّينٌ",
        "translationKey" : "Al-Mutaffifin_8_Translation",
        "transliterationKey" : "Al-Mutaffifin_8_Transliteration",
        "audioFile" : "Al-Mutaffifin_8"
    },
    {
        "text" : "كِتَابٌ مَرْقُومٌ",
        "translationKey" : "Al-Mutaffifin_9_Translation",
        "transliterationKey" : "Al-Mutaffifin_9_Transliteration",
        "audioFile" : "Al-Mutaffifin_9"
    },
    {
        "text" : "وَيْلٌ يَوْمَئِذٍ لِلْمُكَذِّبِينَ",
        "translationKey" : "Al-Mutaffifin_10_Translation",
        "transliterationKey" : "Al-Mutaffifin_10_Transliteration",
        "audioFile" : "Al-Mutaffifin_10"
    },
    {
        "text" : "الَّذِينَ يُكَذِّبُونَ بِيَوْمِ الدِّينِ",
        "translationKey" : "Al-Mutaffifin_11_Translation",
        "transliterationKey" : "Al-Mutaffifin_11_Transliteration",
        "audioFile" : "Al-Mutaffifin_11"
    },
    {
        "text" : "وَمَا يُكَذِّبُ بِهِ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ",
        "translationKey" : "Al-Mutaffifin_12_Translation",
        "transliterationKey" : "Al-Mutaffifin_12_Transliteration",
        "audioFile" : "Al-Mutaffifin_12"
    },
    {
        "text" : "إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ",
        "translationKey" : "Al-Mutaffifin_13_Translation",
        "transliterationKey" : "Al-Mutaffifin_13_Transliteration",
        "audioFile" : "Al-Mutaffifin_13"
    },
    {
        "text" : "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِمْ مَا كَانُوا يَكْسِبُونَ",
        "translationKey" : "Al-Mutaffifin_14_Translation",
        "transliterationKey" : "Al-Mutaffifin_14_Transliteration",
        "audioFile" : "Al-Mutaffifin_14"
    },
    {
        "text" : "كَلَّا إِنَّهُمْ عَنْ رَبِّهِمْ يَوْمَئِذٍ لَمَحْجُوبُونَ",
        "translationKey" : "Al-Mutaffifin_15_Translation",
        "transliterationKey" : "Al-Mutaffifin_15_Transliteration",
        "audioFile" : "Al-Mutaffifin_15"
    },
    {
        "text" : "ثُمَّ إِنَّهُمْ لَصَالُو الْجَحِيمِ",
        "translationKey" : "Al-Mutaffifin_16_Translation",
        "transliterationKey" : "Al-Mutaffifin_16_Transliteration",
        "audioFile" : "Al-Mutaffifin_16"
    },
    {
        "text" : "ثُمَّ يُقَالُ هَٰذَا الَّذِي كُنْتُمْ بِهِ تُكَذِّبُونَ",
        "translationKey" : "Al-Mutaffifin_17_Translation",
        "transliterationKey" : "Al-Mutaffifin_17_Transliteration",
        "audioFile" : "Al-Mutaffifin_17"
    },
    
    {
        "text" : "كَلَّا إِنَّ كِتَابَ الْأَبْرَارِ لَفِي عِلِّيِّينَ",
        "translationKey" : "Al-Mutaffifin_18_Translation",
        "transliterationKey" : "Al-Mutaffifin_18_Transliteration",
        "audioFile" : "Al-Mutaffifin_18"
    },
    {
        "text" : "وَمَا أَدْرَاكَ مَا عِلِّيُّونَ",
        "translationKey" : "Al-Mutaffifin_19_Translation",
        "transliterationKey" : "Al-Mutaffifin_19_Transliteration",
        "audioFile" : "Al-Mutaffifin_19"
    },
    {
        "text" : "كِتَابٌ مَرْقُومٌ",
        "translationKey" : "Al-Mutaffifin_20_Translation",
        "transliterationKey" : "Al-Mutaffifin_20_Transliteration",
        "audioFile" : "Al-Mutaffifin_20"
    },
    {
        "text" : "يَشْهَدُهُ الْمُقَرَّبُونَ",
        "translationKey" : "Al-Mutaffifin_21_Translation",
        "transliterationKey" : "Al-Mutaffifin_21_Transliteration",
        "audioFile" : "Al-Mutaffifin_21"
    },
    {
        "text" : "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ",
        "translationKey" : "Al-Mutaffifin_22_Translation",
        "transliterationKey" : "Al-Mutaffifin_22_Transliteration",
        "audioFile" : "Al-Mutaffifin_22"
    },
    {
        "text" : "عَلَى الْأَرَائِكِ يَنْظُرُونَ",
        "translationKey" : "Al-Mutaffifin_23_Translation",
        "transliterationKey" : "Al-Mutaffifin_23_Transliteration",
        "audioFile" : "Al-Mutaffifin_23"
    },
    {
        "text" : "تَعْرِفُ فِي وُجُوهِهِمْ نَضْرَةَ النَّعِيمِ",
        "translationKey" : "Al-Mutaffifin_24_Translation",
        "transliterationKey" : "Al-Mutaffifin_24_Transliteration",
        "audioFile" : "Al-Mutaffifin_24"
    },
    {
        "text" : "يُسْقَوْنَ مِنْ رَحِيقٍ مَخْتُومٍ",
        "translationKey" : "Al-Mutaffifin_25_Translation",
        "transliterationKey" : "Al-Mutaffifin_25_Transliteration",
        "audioFile" : "Al-Mutaffifin_25"
    },
    {
        "text" : "خِتَامُهُ مِسْكٌ ۚ وَفِي ذَٰلِكَ فَلْيَتَنَافَسِ الْمُتَنَافِسُونَ",
        "translationKey" : "Al-Mutaffifin_26_Translation",
        "transliterationKey" : "Al-Mutaffifin_26_Transliteration",
        "audioFile" : "Al-Mutaffifin_26"
    },
    {
        "text" : "وَمِزَاجُهُ مِنْ تَسْنِيمٍ",
        "translationKey" : "Al-Mutaffifin_27_Translation",
        "transliterationKey" : "Al-Mutaffifin_27_Transliteration",
        "audioFile" : "Al-Mutaffifin_27"
    },
    {
        "text" : "عَيْنًا يَشْرَبُ بِهَا الْمُقَرَّبُونَ",
        "translationKey" : "Al-Mutaffifin_28_Translation",
        "transliterationKey" : "Al-Mutaffifin_28_Transliteration",
        "audioFile" : "Al-Mutaffifin_28"
    },
    {
        "text" : "إِنَّ الَّذِينَ أَجْرَمُوا كَانُوا مِنَ الَّذِينَ آمَنُوا يَضْحَكُونَ",
        "translationKey" : "Al-Mutaffifin_29_Translation",
        "transliterationKey" : "Al-Mutaffifin_29_Transliteration",
        "audioFile" : "Al-Mutaffifin_29"
    },
    {
        "text" : "وَإِذَا مَرُّوا بِهِمْ يَتَغَامَزُونَ",
        "translationKey" : "Al-Mutaffifin_30_Translation",
        "transliterationKey" : "Al-Mutaffifin_30_Transliteration",
        "audioFile" : "Al-Mutaffifin_30"
    },
    {
        "text" : "وَإِذَا انْقَلَبُوا إِلَىٰ أَهْلِهِمُ انْقَلَبُوا فَكِهِينَ",
        "translationKey" : "Al-Mutaffifin_31_Translation",
        "transliterationKey" : "Al-Mutaffifin_31_Transliteration",
        "audioFile" : "Al-Mutaffifin_31"
    },
    {
        "text" : "وَإِذَا رَأَوْهُمْ قَالُوا إِنَّ هَٰؤُلَاءِ لَضَالُّونَ",
        "translationKey" : "Al-Mutaffifin_32_Translation",
        "transliterationKey" : "Al-Mutaffifin_32_Transliteration",
        "audioFile" : "Al-Mutaffifin_32"
    },
    {
        "text" : "وَمَا أُرْسِلُوا عَلَيْهِمْ حَافِظِينَ",
        "translationKey" : "Al-Mutaffifin_33_Translation",
        "transliterationKey" : "Al-Mutaffifin_33_Transliteration",
        "audioFile" : "Al-Mutaffifin_33"
    },
    {
        "text" : "فَالْيَوْمَ الَّذِينَ آمَنُوا مِنَ الْكُفَّارِ يَضْحَكُونَ",
        "translationKey" : "Al-Mutaffifin_34_Translation",
        "transliterationKey" : "Al-Mutaffifin_34_Transliteration",
        "audioFile" : "Al-Mutaffifin_34"
    },
    {
        "text" : "‎عَلَى الْأَرَائِكِ يَنْظُرُونَ",
        "translationKey" : "Al-Mutaffifin_35_Translation",
        "transliterationKey" : "Al-Mutaffifin_35_Transliteration",
        "audioFile" : "Al-Mutaffifin_35"
    },
    {
        "text" : "هَلْ ثُوِّبَ الْكُفَّارُ مَا كَانُوا يَفْعَلُونَ",
        "translationKey" : "Al-Mutaffifin_36_Translation",
        "transliterationKey" : "Al-Mutaffifin_36_Transliteration",
        "audioFile" : "Al-Mutaffifin_36"
    }
]

