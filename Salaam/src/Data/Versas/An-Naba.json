[
    {
        "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "translationKey" : "An-Naba_0_Translation",
        "transliterationKey" : "An-Naba_0_Transliteration",
        "audioFile" : "An-Naba_0"
    },
    {
        "text" : "عَمَّ يَتَسَاءَلُونَ",
        "translationKey" : "An-Naba_1_Translation",
        "transliterationKey" : "An-Naba_1_Transliteration",
        "audioFile" : "An-Naba_1"
    },
    {
        "text" : "عَنِ النَّبَإِ الْعَظِيمِ",
        "translationKey" : "An-Naba_2_Translation",
        "transliterationKey" : "An-Naba_2_Transliteration",
        "audioFile" : "An-Naba_2"
    },
    {
        "text" : "الَّذِي هُمْ فِيهِ مُخْتَلِفُونَ",
        "translationKey" : "An-Naba_3_Translation",
        "transliterationKey" : "An-Naba_3_Transliteration",
        "audioFile" : "An-Naba_3"
    },
    {
        "text" : "كَلَّا سَيَعْلَمُونَ",
        "translationKey" : "An-Naba_4_Translation",
        "transliterationKey" : "An-Naba_4_Transliteration",
        "audioFile" : "An-Naba_4"
    },
    {
        "text" : "ثُمَّ كَلَّا سَيَعْلَمُونَ",
        "translationKey" : "An-Naba_5_Translation",
        "transliterationKey" : "An-Naba_5_Transliteration",
        "audioFile" : "An-Naba_5"
    },
    {
        "text" : "أَلَمْ نَجْعَلِ الْأَرْضَ مِهَادًا",
        "translationKey" : "An-Naba_6_Translation",
        "transliterationKey" : "An-Naba_6_Transliteration",
        "audioFile" : "An-Naba_6"
    },
    {
        "text" : "وَالْجِبَالَ أَوْتَادًا",
        "translationKey" : "An-Naba_7_Translation",
        "transliterationKey" : "An-Naba_7_Transliteration",
        "audioFile" : "An-Naba_7"
    },
    {
        "text" : "وَخَلَقْنَاكُمْ أَزْوَاجًا",
        "translationKey" : "An-Naba_8_Translation",
        "transliterationKey" : "An-Naba_8_Transliteration",
        "audioFile" : "An-Naba_8"
    },
    
    {
        "text" : "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا",
        "translationKey" : "An-Naba_9_Translation",
        "transliterationKey" : "An-Naba_9_Transliteration",
        "audioFile" : "An-Naba_9"
    },
    {
        "text" : "وَجَعَلْنَا اللَّيْلَ لِبَاسًا",
        "translationKey" : "An-Naba_10_Translation",
        "transliterationKey" : "An-Naba_10_Transliteration",
        "audioFile" : "An-Naba_10"
    },
    {
        "text" : "وَجَعَلْنَا النَّهَارَ مَعَاشًا",
        "translationKey" : "An-Naba_11_Translation",
        "transliterationKey" : "An-Naba_11_Transliteration",
        "audioFile" : "An-Naba_11"
    },
    {
        "text" : "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا",
        "translationKey" : "An-Naba_12_Translation",
        "transliterationKey" : "An-Naba_12_Transliteration",
        "audioFile" : "An-Naba_12"
    },
    {
        "text" : "وَجَعَلْنَا سِرَاجًا وَهَّاجًا",
        "translationKey" : "An-Naba_13_Translation",
        "transliterationKey" : "An-Naba_13_Transliteration",
        "audioFile" : "An-Naba_13"
    },
    {
        "text" : "وَأَنْزَلْنَا مِنَ الْمُعْصِرَاتِ مَاءً ثَجَّاجًا",
        "translationKey" : "An-Naba_14_Translation",
        "transliterationKey" : "An-Naba_14_Transliteration",
        "audioFile" : "An-Naba_14"
    },
    {
        "text" : "لِنُخْرِجَ بِهِ حَبًّا وَنَبَاتًا",
        "translationKey" : "An-Naba_15_Translation",
        "transliterationKey" : "An-Naba_15_Transliteration",
        "audioFile" : "An-Naba_15"
    },
    {
        "text" : "وَجَنَّاتٍ أَلْفَافًا",
        "translationKey" : "An-Naba_16_Translation",
        "transliterationKey" : "An-Naba_16_Transliteration",
        "audioFile" : "An-Naba_16"
    },
    {
        "text" : "إِنَّ يَوْمَ الْفَصْلِ كَانَ مِيقَاتًا",
        "translationKey" : "An-Naba_17_Translation",
        "transliterationKey" : "An-Naba_17_Transliteration",
        "audioFile" : "An-Naba_17"
    },
    
    {
        "text" : "يَوْمَ يُنْفَخُ فِي الصُّورِ فَتَأْتُونَ أَفْوَاجًا",
        "translationKey" : "An-Naba_18_Translation",
        "transliterationKey" : "An-Naba_18_Transliteration",
        "audioFile" : "An-Naba_18"
    },
    {
        "text" : "وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَابًا",
        "translationKey" : "An-Naba_19_Translation",
        "transliterationKey" : "An-Naba_19_Transliteration",
        "audioFile" : "An-Naba_19"
    },
    {
        "text" : "وَسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا",
        "translationKey" : "An-Naba_20_Translation",
        "transliterationKey" : "An-Naba_20_Transliteration",
        "audioFile" : "An-Naba_20"
    },
    {
        "text" : "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا",
        "translationKey" : "An-Naba_21_Translation",
        "transliterationKey" : "An-Naba_21_Transliteration",
        "audioFile" : "An-Naba_21"
    },
    {
        "text" : "لِلطَّاغِينَ مَآبًا",
        "translationKey" : "An-Naba_22_Translation",
        "transliterationKey" : "An-Naba_22_Transliteration",
        "audioFile" : "An-Naba_22"
    },
    {
        "text" : "لَابِثِينَ فِيهَا أَحْقَابًا",
        "translationKey" : "An-Naba_23_Translation",
        "transliterationKey" : "An-Naba_23_Transliteration",
        "audioFile" : "An-Naba_23"
    },
    {
        "text" : "لَا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا",
        "translationKey" : "An-Naba_24_Translation",
        "transliterationKey" : "An-Naba_24_Transliteration",
        "audioFile" : "An-Naba_24"
    },
    {
        "text" : "إِلَّا حَمِيمًا وَغَسَّاقًا",
        "translationKey" : "An-Naba_25_Translation",
        "transliterationKey" : "An-Naba_25_Transliteration",
        "audioFile" : "An-Naba_25"
    },
    {
        "text" : "جَزَاءً وِفَاقًا",
        "translationKey" : "An-Naba_26_Translation",
        "transliterationKey" : "An-Naba_26_Transliteration",
        "audioFile" : "An-Naba_26"
    },
    {
        "text" : "إِنَّهُمْ كَانُوا لَا يَرْجُونَ حِسَابًا",
        "translationKey" : "An-Naba_27_Translation",
        "transliterationKey" : "An-Naba_27_Transliteration",
        "audioFile" : "An-Naba_27"
    },
    {
        "text" : "وَكَذَّبُوا بِآيَاتِنَا كِذَّابًا",
        "translationKey" : "An-Naba_28_Translation",
        "transliterationKey" : "An-Naba_28_Transliteration",
        "audioFile" : "An-Naba_28"
    },
    {
        "text" : "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ كِتَابًا",
        "translationKey" : "An-Naba_29_Translation",
        "transliterationKey" : "An-Naba_29_Transliteration",
        "audioFile" : "An-Naba_29"
    },
    {
        "text" : "فَذُوقُوا فَلَنْ نَزِيدَكُمْ إِلَّا عَذَابًا",
        "translationKey" : "An-Naba_30_Translation",
        "transliterationKey" : "An-Naba_30_Transliteration",
        "audioFile" : "An-Naba_30"
    },
    {
        "text" : "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
        "translationKey" : "An-Naba_31_Translation",
        "transliterationKey" : "An-Naba_31_Transliteration",
        "audioFile" : "An-Naba_31"
    },
    {
        "text" : "حَدَائِقَ وَأَعْنَابًا",
        "translationKey" : "An-Naba_32_Translation",
        "transliterationKey" : "An-Naba_32_Transliteration",
        "audioFile" : "An-Naba_32"
    },
    {
        "text" : "وَكَوَاعِبَ أَتْرَابًا",
        "translationKey" : "An-Naba_33_Translation",
        "transliterationKey" : "An-Naba_33_Transliteration",
        "audioFile" : "An-Naba_33"
    },
    {
        "text" : "وَكَأْسًا دِهَاقًا",
        "translationKey" : "An-Naba_34_Translation",
        "transliterationKey" : "An-Naba_34_Transliteration",
        "audioFile" : "An-Naba_34"
    },
    {
        "text" : "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّابًا",
        "translationKey" : "An-Naba_35_Translation",
        "transliterationKey" : "An-Naba_35_Transliteration",
        "audioFile" : "An-Naba_35"
    },
    {
        "text" : "جَزَاءً مِنْ رَبِّكَ عَطَاءً حِسَابًا",
        "translationKey" : "An-Naba_36_Translation",
        "transliterationKey" : "An-Naba_36_Transliteration",
        "audioFile" : "An-Naba_36"
    },
    {
        "text" : "رَبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا",
        "translationKey" : "An-Naba_37_Translation",
        "transliterationKey" : "An-Naba_37_Transliteration",
        "audioFile" : "An-Naba_37"
    },
    {
        "text" : "يَوْمَ يَقُومُ الرُّوحُ وَالْمَلَائِكَةُ صَفًّا ۖ لَا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ الرَّحْمَٰنُ وَقَالَ صَوَابًا",
        "translationKey" : "An-Naba_38_Translation",
        "transliterationKey" : "An-Naba_38_Transliteration",
        "audioFile" : "An-Naba_38"
    },
    {
        "text" : "ذَٰلِكَ الْيَوْمُ الْحَقُّ ۖ فَمَنْ شَاءَ اتَّخَذَ إِلَىٰ رَبِّهِ مَآبًا",
        "translationKey" : "An-Naba_39_Translation",
        "transliterationKey" : "An-Naba_39_Transliteration",
        "audioFile" : "An-Naba_39"
    },
    {
        "text" : "إِنَّا أَنْذَرْنَاكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنْظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ الْكَافِرُ يَا لَيْتَنِي كُنْتُ تُرَابًا",
        "translationKey" : "An-Naba_40_Translation",
        "transliterationKey" : "An-Naba_40_Transliteration",
        "audioFile" : "An-Naba_40"
    }
]

