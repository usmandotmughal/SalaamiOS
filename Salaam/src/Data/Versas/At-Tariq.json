[
  {
    "audioFile" : "At-Tariq_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "At-Tariq_0_Translation",
    "transliterationKey" : "At-Tariq_0_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_1",
    "text" : "وَالسَّمَاءِ وَالطَّارِقِ",
    "translationKey" : "At-Tariq_1_Translation",
    "transliterationKey" : "At-Tariq_1_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_2",
    "text" : "وَمَا أَدْرَاكَ مَا الطَّارِقُ",
    "translationKey" : "At-Tariq_2_Translation",
    "transliterationKey" : "At-Tariq_2_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_3",
    "text" : "النَّجْمُ الثَّاقِبُ",
    "translationKey" : "At-Tariq_3_Translation",
    "transliterationKey" : "At-Tariq_3_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_4",
    "text" : "إِنْ كُلُّ نَفْسٍ لَمَّا عَلَيْهَا حَافِظٌ",
    "translationKey" : "At-Tariq_4_Translation",
    "transliterationKey" : "At-Tariq_4_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_5",
    "text" : "فَلْيَنْظُرِ الْإِنْسَانُ مِمَّ خُلِقَ",
    "translationKey" : "At-Tariq_5_Translation",
    "transliterationKey" : "At-Tariq_5_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_6",
    "text" : "خُلِقَ مِنْ مَاءٍ دَافِقٍ",
    "translationKey" : "At-Tariq_6_Translation",
    "transliterationKey" : "At-Tariq_6_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_7",
    "text" : "يَخْرُجُ مِنْ بَيْنِ الصُّلْبِ وَالتَّرَائِبِ",
    "translationKey" : "At-Tariq_7_Translation",
    "transliterationKey" : "At-Tariq_7_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_8",
    "text" : "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ",
    "translationKey" : "At-Tariq_8_Translation",
    "transliterationKey" : "At-Tariq_8_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_9",
    "text" : "يَوْمَ تُبْلَى السَّرَائِرُ",
    "translationKey" : "At-Tariq_9_Translation",
    "transliterationKey" : "At-Tariq_9_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_10",
    "text" : "فَمَا لَهُ مِنْ قُوَّةٍ وَلَا نَاصِرٍ",
    "translationKey" : "At-Tariq_10_Translation",
    "transliterationKey" : "At-Tariq_10_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_11",
    "text" : "وَالسَّمَاءِ ذَاتِ الرَّجْعِ",
    "translationKey" : "At-Tariq_11_Translation",
    "transliterationKey" : "At-Tariq_11_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_12",
    "text" : "وَالْأَرْضِ ذَاتِ الصَّدْعِ",
    "translationKey" : "At-Tariq_12_Translation",
    "transliterationKey" : "At-Tariq_12_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_13",
    "text" : "إِنَّهُ لَقَوْلٌ فَصْلٌ",
    "translationKey" : "At-Tariq_13_Translation",
    "transliterationKey" : "At-Tariq_13_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_14",
    "text" : "وَمَا هُوَ بِالْهَزْلِ",
    "translationKey" : "At-Tariq_14_Translation",
    "transliterationKey" : "At-Tariq_14_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_15",
    "text" : "إِنَّهُمْ يَكِيدُونَ كَيْدًا",
    "translationKey" : "At-Tariq_15_Translation",
    "transliterationKey" : "At-Tariq_15_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_16",
    "text" : "وَأَكِيدُ كَيْدًا",
    "translationKey" : "At-Tariq_16_Translation",
    "transliterationKey" : "At-Tariq_16_Transliteration"
  },
  {
    "audioFile" : "At-Tariq_17",
    "text" : "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا",
    "translationKey" : "At-Tariq_17_Translation",
    "transliterationKey" : "At-Tariq_17_Transliteration"
  }
]