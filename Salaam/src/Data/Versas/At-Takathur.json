        "transliterationKey" : "At-Takathur_4_Transliteration",
        "audioFile" : "At-Takathur_4"
    },
    {
        "text" : "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ الْيَقِينِ",
        "translationKey" : "At-Takathur_5_Translation",
        "transliterationKey" : "At-Takathur_5_Transliteration",
        "audioFile" : "At-Takathur_5"
    },
    {
