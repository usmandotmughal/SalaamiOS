[
  {
    "audioFile" : "Al-'Alaq_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Al-'Alaq_0_Translation",
    "transliterationKey" : "Al-'Alaq_0_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_1",
    "text" : "اقْرَأْ بِاسْمِ رَبِّكَ الَّذِي خَلَقَ",
    "translationKey" : "Al-'Alaq_1_Translation",
    "transliterationKey" : "Al-'Alaq_1_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_2",
    "text" : "‎خَلَقَ الْإِنْسَانَ مِنْ عَلَقٍ",
    "translationKey" : "Al-'Alaq_2_Translation",
    "transliterationKey" : "Al-'Alaq_2_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_3",
    "text" : "‎اقْرَأْ وَرَبُّكَ الْأَكْرَمُ",
    "translationKey" : "Al-'Alaq_3_Translation",
    "transliterationKey" : "Al-'Alaq_3_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_4",
    "text" : "‎الَّذِي عَلَّمَ بِالْقَلَمِ",
    "translationKey" : "Al-'Alaq_4_Translation",
    "transliterationKey" : "Al-'Alaq_4_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_5",
    "text" : "‎عَلَّمَ الْإِنْسَانَ مَا لَمْ يَعْلَمْ",
    "translationKey" : "Al-'Alaq_5_Translation",
    "transliterationKey" : "Al-'Alaq_5_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_6",
    "text" : "‎كَلَّا إِنَّ الْإِنْسَانَ لَيَطْغَىٰ",
    "translationKey" : "Al-'Alaq_6_Translation",
    "transliterationKey" : "Al-'Alaq_6_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_7",
    "text" : "‎أَنْ رَآهُ اسْتَغْنَىٰ",
    "translationKey" : "Al-'Alaq_7_Translation",
    "transliterationKey" : "Al-'Alaq_7_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_8",
    "text" : "‎إِنَّ إِلَىٰ رَبِّكَ الرُّجْعَىٰ",
    "translationKey" : "Al-'Alaq_8_Translation",
    "transliterationKey" : "Al-'Alaq_8_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_9",
    "text" : "‎أَرَأَيْتَ الَّذِي يَنْهَىٰ",
    "translationKey" : "Al-'Alaq_9_Translation",
    "transliterationKey" : "Al-'Alaq_9_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_10",
    "text" : "‎عَبْدًا إِذَا صَلَّىٰ",
    "translationKey" : "Al-'Alaq_10_Translation",
    "transliterationKey" : "Al-'Alaq_10_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_11",
    "text" : "‎أَرَأَيْتَ إِنْ كَانَ عَلَى الْهُدَىٰ",
    "translationKey" : "Al-'Alaq_11_Translation",
    "transliterationKey" : "Al-'Alaq_11_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_12",
    "text" : "‎أَوْ أَمَرَ بِالتَّقْوَىٰ",
    "translationKey" : "Al-'Alaq_12_Translation",
    "transliterationKey" : "Al-'Alaq_12_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_13",
    "text" : "‎أَرَأَيْتَ إِنْ كَذَّبَ وَتَوَلَّىٰ",
    "translationKey" : "Al-'Alaq_13_Translation",
    "transliterationKey" : "Al-'Alaq_13_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_14",
    "text" : "‎أَلَمْ يَعْلَمْ بِأَنَّ اللَّهَ يَرَىٰ",
    "translationKey" : "Al-'Alaq_14_Translation",
    "transliterationKey" : "Al-'Alaq_14_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_15",
    "text" : "‎كَلَّا لَئِنْ لَمْ يَنْتَهِ لَنَسْفَعًا بِالنَّاصِيَةِ",
    "translationKey" : "Al-'Alaq_15_Translation",
    "transliterationKey" : "Al-'Alaq_15_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_16",
    "text" : "‎نَاصِيَةٍ كَاذِبَةٍ خَاطِئَةٍ",
    "translationKey" : "Al-'Alaq_16_Translation",
    "transliterationKey" : "Al-'Alaq_16_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_17",
    "text" : "‎فَلْيَدْعُ نَادِيَهُ",
    "translationKey" : "Al-'Alaq_17_Translation",
    "transliterationKey" : "Al-'Alaq_17_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_18",
    "text" : "‎سَنَدْعُ الزَّبَانِيَةَ",
    "translationKey" : "Al-'Alaq_18_Translation",
    "transliterationKey" : "Al-'Alaq_18_Transliteration"
  },
  {
    "audioFile" : "Al-'Alaq_19",
    "text" : "‎۩ كَلَّا لَا تُطِعْهُ وَاسْجُدْ وَاقْتَرِبْ ",
    "translationKey" : "Al-'Alaq_19_Translation",
    "transliterationKey" : "Al-'Alaq_19_Transliteration"
  }
]
