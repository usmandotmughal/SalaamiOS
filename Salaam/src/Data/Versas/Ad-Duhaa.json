[
  {
    "audioFile" : "Ad-Duhaa_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Ad-Duhaa_0_Translation",
    "transliterationKey" : "Ad-Duhaa_0_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_1",
    "text" : "وَالضُّحَىٰ",
    "translationKey" : "Ad-Duhaa_1_Translation",
    "transliterationKey" : "Ad-Duhaa_1_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_2",
    "text" : "وَاللَّيْلِ إِذَا سَجَىٰ",
    "translationKey" : "Ad-Duhaa_2_Translation",
    "transliterationKey" : "Ad-Duhaa_2_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_3",
    "text" : "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
    "translationKey" : "Ad-Duhaa_3_Translation",
    "transliterationKey" : "Ad-Duhaa_3_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_4",
    "text" : "وَلَلْآخِرَةُ خَيْرٌ لَكَ مِنَ الْأُولَىٰ",
    "translationKey" : "Ad-Duhaa_4_Translation",
    "transliterationKey" : "Ad-Duhaa_4_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_5",
    "text" : "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰ",
    "translationKey" : "Ad-Duhaa_5_Translation",
    "transliterationKey" : "Ad-Duhaa_5_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_6",
    "text" : "أَلَمْ يَجِدْكَ يَتِيمًا فَآوَىٰ",
    "translationKey" : "Ad-Duhaa_6_Translation",
    "transliterationKey" : "Ad-Duhaa_6_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_7",
    "text" : "وَوَجَدَكَ ضَالًّا فَهَدَىٰ",
    "translationKey" : "Ad-Duhaa_7_Translation",
    "transliterationKey" : "Ad-Duhaa_7_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_8",
    "text" : "وَوَجَدَكَ عَائِلًا فَأَغْنَىٰ",
    "translationKey" : "Ad-Duhaa_8_Translation",
    "transliterationKey" : "Ad-Duhaa_8_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_9",
    "text" : "فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ",
    "translationKey" : "Ad-Duhaa_9_Translation",
    "transliterationKey" : "Ad-Duhaa_9_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_10",
    "text" : "وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ",
    "translationKey" : "Ad-Duhaa_10_Translation",
    "transliterationKey" : "Ad-Duhaa_10_Transliteration"
  },
  {
    "audioFile" : "Ad-Duhaa_11",
    "text" : "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ",
    "translationKey" : "Ad-Duhaa_11_Translation",
    "transliterationKey" : "Ad-Duhaa_11_Transliteration"
  }
]