[
  {
    "audioFile" : "Al-Ghashiyah_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Al-Ghashiyah_0_Translation",
    "transliterationKey" : "Al-Ghashiyah_0_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_1",
    "text" : "هَلْ أَتَاكَ حَدِيثُ الْغَاشِيَةِ",
    "translationKey" : "Al-Ghashiyah_1_Translation",
    "transliterationKey" : "Al-Ghashiyah_1_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_2",
    "text" : "وُجُوهٌ يَوْمَئِذٍ خَاشِعَةٌ",
    "translationKey" : "Al-Ghashiyah_2_Translation",
    "transliterationKey" : "Al-Ghashiyah_2_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_3",
    "text" : "عَامِلَةٌ نَاصِبَةٌ",
    "translationKey" : "Al-Ghashiyah_3_Translation",
    "transliterationKey" : "Al-Ghashiyah_3_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_4",
    "text" : "تَصْلَىٰ نَارًا حَامِيَةً",
    "translationKey" : "Al-Ghashiyah_4_Translation",
    "transliterationKey" : "Al-Ghashiyah_4_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_5",
    "text" : "تُسْقَىٰ مِنْ عَيْنٍ آنِيَةٍ",
    "translationKey" : "Al-Ghashiyah_5_Translation",
    "transliterationKey" : "Al-Ghashiyah_5_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_6",
    "text" : "لَيْسَ لَهُمْ طَعَامٌ إِلَّا مِنْ ضَرِيعٍ",
    "translationKey" : "Al-Ghashiyah_6_Translation",
    "transliterationKey" : "Al-Ghashiyah_6_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_7",
    "text" : "لَا يُسْمِنُ وَلَا يُغْنِي مِنْ جُوعٍ",
    "translationKey" : "Al-Ghashiyah_7_Translation",
    "transliterationKey" : "Al-Ghashiyah_7_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_8",
    "text" : "وُجُوهٌ يَوْمَئِذٍ نَاعِمَةٌ",
    "translationKey" : "Al-Ghashiyah_8_Translation",
    "transliterationKey" : "Al-Ghashiyah_8_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_9",
    "text" : "لِسَعْيِهَا رَاضِيَةٌ",
    "translationKey" : "Al-Ghashiyah_9_Translation",
    "transliterationKey" : "Al-Ghashiyah_9_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_10",
    "text" : "فِي جَنَّةٍ عَالِيَةٍ",
    "translationKey" : "Al-Ghashiyah_10_Translation",
    "transliterationKey" : "Al-Ghashiyah_10_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_11",
    "text" : "لَا تَسْمَعُ فِيهَا لَاغِيَةً",
    "translationKey" : "Al-Ghashiyah_11_Translation",
    "transliterationKey" : "Al-Ghashiyah_11_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_12",
    "text" : "فِيهَا عَيْنٌ جَارِيَةٌ",
    "translationKey" : "Al-Ghashiyah_12_Translation",
    "transliterationKey" : "Al-Ghashiyah_12_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_13",
    "text" : "فِيهَا سُرُرٌ مَرْفُوعَةٌ",
    "translationKey" : "Al-Ghashiyah_13_Translation",
    "transliterationKey" : "Al-Ghashiyah_13_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_14",
    "text" : "وَأَكْوَابٌ مَوْضُوعَةٌ",
    "translationKey" : "Al-Ghashiyah_14_Translation",
    "transliterationKey" : "Al-Ghashiyah_14_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_15",
    "text" : "وَنَمَارِقُ مَصْفُوفَةٌ",
    "translationKey" : "Al-Ghashiyah_15_Translation",
    "transliterationKey" : "Al-Ghashiyah_15_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_16",
    "text" : "وَزَرَابِيُّ مَبْثُوثَةٌ",
    "translationKey" : "Al-Ghashiyah_16_Translation",
    "transliterationKey" : "Al-Ghashiyah_16_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_17",
    "text" : "أَفَلَا يَنْظُرُونَ إِلَى الْإِبِلِ كَيْفَ خُلِقَتْ",
    "translationKey" : "Al-Ghashiyah_17_Translation",
    "transliterationKey" : "Al-Ghashiyah_17_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_18",
    "text" : "وَإِلَى السَّمَاءِ كَيْفَ رُفِعَتْ",
    "translationKey" : "Al-Ghashiyah_18_Translation",
    "transliterationKey" : "Al-Ghashiyah_18_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_19",
    "text" : "وَإِلَى الْجِبَالِ كَيْفَ نُصِبَتْ",
    "translationKey" : "Al-Ghashiyah_19_Translation",
    "transliterationKey" : "Al-Ghashiyah_19_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_20",
    "text" : "وَإِلَى الْأَرْضِ كَيْفَ سُطِحَتْ",
    "translationKey" : "Al-Ghashiyah_20_Translation",
    "transliterationKey" : "Al-Ghashiyah_20_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_21",
    "text" : "فَذَكِّرْ إِنَّمَا أَنْتَ مُذَكِّرٌ",
    "translationKey" : "Al-Ghashiyah_21_Translation",
    "transliterationKey" : "Al-Ghashiyah_21_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_22",
    "text" : "لَسْتَ عَلَيْهِمْ بِمُصَيْطِرٍ",
    "translationKey" : "Al-Ghashiyah_22_Translation",
    "transliterationKey" : "Al-Ghashiyah_22_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_23",
    "text" : "إِلَّا مَنْ تَوَلَّىٰ وَكَفَرَ",
    "translationKey" : "Al-Ghashiyah_23_Translation",
    "transliterationKey" : "Al-Ghashiyah_23_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_24",
    "text" : "فَيُعَذِّبُهُ اللَّهُ الْعَذَابَ الْأَكْبَرَ",
    "translationKey" : "Al-Ghashiyah_24_Translation",
    "transliterationKey" : "Al-Ghashiyah_24_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_25",
    "text" : "إِنَّ إِلَيْنَا إِيَابَهُمْ",
    "translationKey" : "Al-Ghashiyah_25_Translation",
    "transliterationKey" : "Al-Ghashiyah_25_Transliteration"
  },
  {
    "audioFile" : "Al-Ghashiyah_26",
    "text" : "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُمْ",
    "translationKey" : "Al-Ghashiyah_26_Translation",
    "transliterationKey" : "Al-Ghashiyah_26_Transliteration"
  }
]