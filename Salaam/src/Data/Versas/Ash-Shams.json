[
  {
    "audioFile" : "Ash-Shams_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Ash-Shams_0_Translation",
    "transliterationKey" : "Ash-Shams_0_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_1",
    "text" : "وَالشَّمْسِ وَضُحَاهَا",
    "translationKey" : "Ash-Shams_1_Translation",
    "transliterationKey" : "Ash-Shams_1_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_2",
    "text" : "وَالْقَمَرِ إِذَا تَلَاهَا",
    "translationKey" : "Ash-Shams_2_Translation",
    "transliterationKey" : "Ash-Shams_2_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_3",
    "text" : "وَالنَّهَارِ إِذَا جَلَّاهَا",
    "translationKey" : "Ash-Shams_3_Translation",
    "transliterationKey" : "Ash-Shams_3_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_4",
    "text" : "وَاللَّيْلِ إِذَا يَغْشَاهَا",
    "translationKey" : "Ash-Shams_4_Translation",
    "transliterationKey" : "Ash-Shams_4_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_5",
    "text" : "وَالسَّمَاءِ وَمَا بَنَاهَا",
    "translationKey" : "Ash-Shams_5_Translation",
    "transliterationKey" : "Ash-Shams_5_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_6",
    "text" : "وَالْأَرْضِ وَمَا طَحَاهَا",
    "translationKey" : "Ash-Shams_6_Translation",
    "transliterationKey" : "Ash-Shams_6_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_7",
    "text" : "وَنَفْسٍ وَمَا سَوَّاهَا",
    "translationKey" : "Ash-Shams_7_Translation",
    "transliterationKey" : "Ash-Shams_7_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_8",
    "text" : "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَاهَا",
    "translationKey" : "Ash-Shams_8_Translation",
    "transliterationKey" : "Ash-Shams_8_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_9",
    "text" : "قَدْ أَفْلَحَ مَنْ زَكَّاهَا",
    "translationKey" : "Ash-Shams_9_Translation",
    "transliterationKey" : "Ash-Shams_9_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_10",
    "text" : "وَقَدْ خَابَ مَنْ دَسَّاهَا",
    "translationKey" : "Ash-Shams_10_Translation",
    "transliterationKey" : "Ash-Shams_10_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_11",
    "text" : "كَذَّبَتْ ثَمُودُ بِطَغْوَاهَا",
    "translationKey" : "Ash-Shams_11_Translation",
    "transliterationKey" : "Ash-Shams_11_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_12",
    "text" : "إِذِ انْبَعَثَ أَشْقَاهَا",
    "translationKey" : "Ash-Shams_12_Translation",
    "transliterationKey" : "Ash-Shams_12_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_13",
    "text" : "فَقَالَ لَهُمْ رَسُولُ اللَّهِ نَاقَةَ اللَّهِ وَسُقْيَاهَا",
    "translationKey" : "Ash-Shams_13_Translation",
    "transliterationKey" : "Ash-Shams_13_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_14",
    "text" : "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُمْ بِذَنْبِهِمْ فَسَوَّاهَا",
    "translationKey" : "Ash-Shams_14_Translation",
    "transliterationKey" : "Ash-Shams_14_Transliteration"
  },
  {
    "audioFile" : "Ash-Shams_15",
    "text" : "وَلَا يَخَافُ عُقْبَاهَا",
    "translationKey" : "Ash-Shams_15_Translation",
    "transliterationKey" : "Ash-Shams_15_Transliteration"
  }
]