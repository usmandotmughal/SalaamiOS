[
  {
    "audioFile" : "Al-Fajr_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Al-Fajr_0_Translation",
    "transliterationKey" : "Al-Fajr_0_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_1",
    "text" : "وَالْفَجْرِ",
    "translationKey" : "Al-Fajr_1_Translation",
    "transliterationKey" : "Al-Fajr_1_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_2",
    "text" : "وَلَيَالٍ عَشْرٍ",
    "translationKey" : "Al-Fajr_2_Translation",
    "transliterationKey" : "Al-Fajr_2_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_3",
    "text" : "وَالشَّفْعِ وَالْوَتْرِ",
    "translationKey" : "Al-Fajr_3_Translation",
    "transliterationKey" : "Al-Fajr_3_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_4",
    "text" : "وَاللَّيْلِ إِذَا يَسْرِ",
    "translationKey" : "Al-Fajr_4_Translation",
    "transliterationKey" : "Al-Fajr_4_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_5",
    "text" : "هَلْ فِي ذَٰلِكَ قَسَمٌ لِذِي حِجْرٍ",
    "translationKey" : "Al-Fajr_5_Translation",
    "transliterationKey" : "Al-Fajr_5_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_6",
    "text" : "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
    "translationKey" : "Al-Fajr_6_Translation",
    "transliterationKey" : "Al-Fajr_6_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_7",
    "text" : "إِرَمَ ذَاتِ الْعِمَادِ",
    "translationKey" : "Al-Fajr_7_Translation",
    "transliterationKey" : "Al-Fajr_7_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_8",
    "text" : "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ",
    "translationKey" : "Al-Fajr_8_Translation",
    "transliterationKey" : "Al-Fajr_8_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_9",
    "text" : "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ",
    "translationKey" : "Al-Fajr_9_Translation",
    "transliterationKey" : "Al-Fajr_9_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_10",
    "text" : "وَفِرْعَوْنَ ذِي الْأَوْتَادِ",
    "translationKey" : "Al-Fajr_10_Translation",
    "transliterationKey" : "Al-Fajr_10_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_11",
    "text" : "الَّذِينَ طَغَوْا فِي الْبِلَادِ",
    "translationKey" : "Al-Fajr_11_Translation",
    "transliterationKey" : "Al-Fajr_11_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_12",
    "text" : "فَأَكْثَرُوا فِيهَا الْفَسَادَ",
    "translationKey" : "Al-Fajr_12_Translation",
    "transliterationKey" : "Al-Fajr_12_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_13",
    "text" : "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
    "translationKey" : "Al-Fajr_13_Translation",
    "transliterationKey" : "Al-Fajr_13_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_14",
    "text" : "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ",
    "translationKey" : "Al-Fajr_14_Translation",
    "transliterationKey" : "Al-Fajr_14_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_15",
    "text" : "فَأَمَّا الْإِنْسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ",
    "translationKey" : "Al-Fajr_15_Translation",
    "transliterationKey" : "Al-Fajr_15_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_16",
    "text" : "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ",
    "translationKey" : "Al-Fajr_16_Translation",
    "transliterationKey" : "Al-Fajr_16_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_17",
    "text" : "كَلَّا ۖ بَلْ لَا تُكْرِمُونَ الْيَتِيمَ",
    "translationKey" : "Al-Fajr_17_Translation",
    "transliterationKey" : "Al-Fajr_17_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_18",
    "text" : "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ",
    "translationKey" : "Al-Fajr_18_Translation",
    "transliterationKey" : "Al-Fajr_18_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_19",
    "text" : "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَمًّا",
    "translationKey" : "Al-Fajr_19_Translation",
    "transliterationKey" : "Al-Fajr_19_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_20",
    "text" : "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا",
    "translationKey" : "Al-Fajr_20_Translation",
    "transliterationKey" : "Al-Fajr_20_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_21",
    "text" : "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا",
    "translationKey" : "Al-Fajr_21_Translation",
    "transliterationKey" : "Al-Fajr_21_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_22",
    "text" : "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا",
    "translationKey" : "Al-Fajr_22_Translation",
    "transliterationKey" : "Al-Fajr_22_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_23",
    "text" : "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنْسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ",
    "translationKey" : "Al-Fajr_23_Translation",
    "transliterationKey" : "Al-Fajr_23_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_24",
    "text" : "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي",
    "translationKey" : "Al-Fajr_24_Translation",
    "transliterationKey" : "Al-Fajr_24_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_25",
    "text" : "فَيَوْمَئِذٍ لَا يُعَذِّبُ عَذَابَهُ أَحَدٌ",
    "translationKey" : "Al-Fajr_25_Translation",
    "transliterationKey" : "Al-Fajr_25_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_26",
    "text" : "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ",
    "translationKey" : "Al-Fajr_26_Translation",
    "transliterationKey" : "Al-Fajr_26_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_27",
    "text" : "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ",
    "translationKey" : "Al-Fajr_27_Translation",
    "transliterationKey" : "Al-Fajr_27_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_28",
    "text" : "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَرْضِيَّةً",
    "translationKey" : "Al-Fajr_28_Translation",
    "transliterationKey" : "Al-Fajr_28_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_29",
    "text" : "فَادْخُلِي فِي عِبَادِي",
    "translationKey" : "Al-Fajr_29_Translation",
    "transliterationKey" : "Al-Fajr_29_Transliteration"
  },
  {
    "audioFile" : "Al-Fajr_30",
    "text" : "وَادْخُلِي جَنَّتِي",
    "translationKey" : "Al-Fajr_30_Translation",
    "transliterationKey" : "Al-Fajr_30_Transliteration"
  }
]