[
    {
        "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "translationKey" : "An-Naziat_0_Translation",
        "transliterationKey" : "An-Naziat_0_Transliteration",
        "audioFile" : "An-Naziat_0"
    },
    {
        "text" : "وَالنَّازِعَاتِ غَرْقًا",
        "translationKey" : "An-Naziat_1_Translation",
        "transliterationKey" : "An-Naziat_1_Transliteration",
        "audioFile" : "An-Naziat_1"
    },
    {
        "text" : "وَالنَّاشِطَاتِ نَشْطًا",
        "translationKey" : "An-Naziat_2_Translation",
        "transliterationKey" : "An-Naziat_2_Transliteration",
        "audioFile" : "An-Naziat_2"
    },
    {
        "text" : "وَالسَّابِحَاتِ سَبْحًا",
        "translationKey" : "An-Naziat_3_Translation",
        "transliterationKey" : "An-Naziat_3_Transliteration",
        "audioFile" : "An-Naziat_3"
    },
    {
        "text" : "فَالسَّابِقَاتِ سَبْقًا",
        "translationKey" : "An-Naziat_4_Translation",
        "transliterationKey" : "An-Naziat_4_Transliteration",
        "audioFile" : "An-Naziat_4"
    },
    {
        "text" : "فَالْمُدَبِّرَاتِ أَمْرًا",
        "translationKey" : "An-Naziat_5_Translation",
        "transliterationKey" : "An-Naziat_5_Transliteration",
        "audioFile" : "An-Naziat_5"
    },
    {
        "text" : "يَوْمَ تَرْجُفُ الرَّاجِفَةُ",
        "translationKey" : "An-Naziat_6_Translation",
        "transliterationKey" : "An-Naziat_6_Transliteration",
        "audioFile" : "An-Naziat_6"
    },
    {
        "text" : "تَتْبَعُهَا الرَّادِفَةُ",
        "translationKey" : "An-Naziat_7_Translation",
        "transliterationKey" : "An-Naziat_7_Transliteration",
        "audioFile" : "An-Naziat_7"
    },
    {
        "text" : "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
        "translationKey" : "An-Naziat_8_Translation",
        "transliterationKey" : "An-Naziat_8_Transliteration",
        "audioFile" : "An-Naziat_8"
    },
    
    {
        "text" : "أَبْصَارُهَا خَاشِعَةٌ",
        "translationKey" : "An-Naziat_9_Translation",
        "transliterationKey" : "An-Naziat_9_Transliteration",
        "audioFile" : "An-Naziat_9"
    },
    {
        "text" : "يَقُولُونَ أَإِنَّا لَمَرْدُودُونَ فِي الْحَافِرَةِ",
        "translationKey" : "An-Naziat_10_Translation",
        "transliterationKey" : "An-Naziat_10_Transliteration",
        "audioFile" : "An-Naziat_10"
    },
    {
        "text" : "أَإِذَا كُنَّا عِظَامًا نَخِرَةً",
        "translationKey" : "An-Naziat_11_Translation",
        "transliterationKey" : "An-Naziat_11_Transliteration",
        "audioFile" : "An-Naziat_11"
    },
    {
        "text" : "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
        "translationKey" : "An-Naziat_12_Translation",
        "transliterationKey" : "An-Naziat_12_Transliteration",
        "audioFile" : "An-Naziat_12"
    },
    {
        "text" : "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ",
        "translationKey" : "An-Naziat_13_Translation",
        "transliterationKey" : "An-Naziat_13_Transliteration",
        "audioFile" : "An-Naziat_13"
    },
    {
        "text" : "فَإِذَا هُمْ بِالسَّاهِرَةِ",
        "translationKey" : "An-Naziat_14_Translation",
        "transliterationKey" : "An-Naziat_14_Transliteration",
        "audioFile" : "An-Naziat_14"
    },
    {
        "text" : "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ",
        "translationKey" : "An-Naziat_15_Translation",
        "transliterationKey" : "An-Naziat_15_Transliteration",
        "audioFile" : "An-Naziat_15"
    },
    {
        "text" : "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى",
        "translationKey" : "An-Naziat_16_Translation",
        "transliterationKey" : "An-Naziat_16_Transliteration",
        "audioFile" : "An-Naziat_16"
    },
    {
        "text" : "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ",
        "translationKey" : "An-Naziat_17_Translation",
        "transliterationKey" : "An-Naziat_17_Transliteration",
        "audioFile" : "An-Naziat_17"
    },
    
    {
        "text" : "فَقُلْ هَلْ لَكَ إِلَىٰ أَنْ تَزَكَّىٰ",
        "translationKey" : "An-Naziat_18_Translation",
        "transliterationKey" : "An-Naziat_18_Transliteration",
        "audioFile" : "An-Naziat_18"
    },
    {
        "text" : "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
        "translationKey" : "An-Naziat_19_Translation",
        "transliterationKey" : "An-Naziat_19_Transliteration",
        "audioFile" : "An-Naziat_19"
    },
    {
        "text" : "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ",
        "translationKey" : "An-Naziat_20_Translation",
        "transliterationKey" : "An-Naziat_20_Transliteration",
        "audioFile" : "An-Naziat_20"
    },
    {
        "text" : "فَكَذَّبَ وَعَصَىٰ",
        "translationKey" : "An-Naziat_21_Translation",
        "transliterationKey" : "An-Naziat_21_Transliteration",
        "audioFile" : "An-Naziat_21"
    },
    {
        "text" : "ثُمَّ أَدْبَرَ يَسْعَىٰ",
        "translationKey" : "An-Naziat_22_Translation",
        "transliterationKey" : "An-Naziat_22_Transliteration",
        "audioFile" : "An-Naziat_22"
    },
    {
        "text" : "فَحَشَرَ فَنَادَىٰ",
        "translationKey" : "An-Naziat_23_Translation",
        "transliterationKey" : "An-Naziat_23_Transliteration",
        "audioFile" : "An-Naziat_23"
    },
    {
        "text" : "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ",
        "translationKey" : "An-Naziat_24_Translation",
        "transliterationKey" : "An-Naziat_24_Transliteration",
        "audioFile" : "An-Naziat_24"
    },
    {
        "text" : "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ",
        "translationKey" : "An-Naziat_25_Translation",
        "transliterationKey" : "An-Naziat_25_Transliteration",
        "audioFile" : "An-Naziat_25"
    },
    {
        "text" : "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِمَنْ يَخْشَىٰ",
        "translationKey" : "An-Naziat_26_Translation",
        "transliterationKey" : "An-Naziat_26_Transliteration",
        "audioFile" : "An-Naziat_26"
    },
    {
        "text" : "أَأَنْتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا",
        "translationKey" : "An-Naziat_27_Translation",
        "transliterationKey" : "An-Naziat_27_Transliteration",
        "audioFile" : "An-Naziat_27"
    },
    {
        "text" : "رَفَعَ سَمْكَهَا فَسَوَّاهَا",
        "translationKey" : "An-Naziat_28_Translation",
        "transliterationKey" : "An-Naziat_28_Transliteration",
        "audioFile" : "An-Naziat_28"
    },
    {
        "text" : "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا",
        "translationKey" : "An-Naziat_29_Translation",
        "transliterationKey" : "An-Naziat_29_Transliteration",
        "audioFile" : "An-Naziat_29"
    },
    {
        "text" : "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا",
        "translationKey" : "An-Naziat_30_Translation",
        "transliterationKey" : "An-Naziat_30_Transliteration",
        "audioFile" : "An-Naziat_30"
    },
    {
        "text" : "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا",
        "translationKey" : "An-Naziat_31_Translation",
        "transliterationKey" : "An-Naziat_31_Transliteration",
        "audioFile" : "An-Naziat_31"
    },
    {
        "text" : "وَالْجِبَالَ أَرْسَاهَا",
        "translationKey" : "An-Naziat_32_Translation",
        "transliterationKey" : "An-Naziat_32_Transliteration",
        "audioFile" : "An-Naziat_32"
    },
    {
        "text" : "مَتَاعًا لَكُمْ وَلِأَنْعَامِكُمْ",
        "translationKey" : "An-Naziat_33_Translation",
        "transliterationKey" : "An-Naziat_33_Transliteration",
        "audioFile" : "An-Naziat_33"
    },
    {
        "text" : "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ",
        "translationKey" : "An-Naziat_34_Translation",
        "transliterationKey" : "An-Naziat_34_Transliteration",
        "audioFile" : "An-Naziat_34"
    },
    {
        "text" : "يَوْمَ يَتَذَكَّرُ الْإِنْسَانُ مَا سَعَىٰ",
        "translationKey" : "An-Naziat_35_Translation",
        "transliterationKey" : "An-Naziat_35_Transliteration",
        "audioFile" : "An-Naziat_35"
    },
    {
        "text" : "وَبُرِّزَتِ الْجَحِيمُ لِمَنْ يَرَىٰ",
        "translationKey" : "An-Naziat_36_Translation",
        "transliterationKey" : "An-Naziat_36_Transliteration",
        "audioFile" : "An-Naziat_36"
    },
    {
        "text" : "فَأَمَّا مَنْ طَغَىٰ",
        "translationKey" : "An-Naziat_37_Translation",
        "transliterationKey" : "An-Naziat_37_Transliteration",
        "audioFile" : "An-Naziat_37"
    },
    {
        "text" : "وَآثَرَ الْحَيَاةَ الدُّنْيَا",
        "translationKey" : "An-Naziat_38_Translation",
        "transliterationKey" : "An-Naziat_38_Transliteration",
        "audioFile" : "An-Naziat_38"
    },
    {
        "text" : "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ",
        "translationKey" : "An-Naziat_39_Translation",
        "transliterationKey" : "An-Naziat_39_Transliteration",
        "audioFile" : "An-Naziat_39"
    },
    {
        "text" : "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ",
        "translationKey" : "An-Naziat_40_Translation",
        "transliterationKey" : "An-Naziat_40_Transliteration",
        "audioFile" : "An-Naziat_40"
    },
    {
        "text" : "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ",
        "translationKey" : "An-Naziat_41_Translation",
        "transliterationKey" : "An-Naziat_41_Transliteration",
        "audioFile" : "An-Naziat_41"
    },
    {
        "text" : "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا",
        "translationKey" : "An-Naziat_42_Translation",
        "transliterationKey" : "An-Naziat_42_Transliteration",
        "audioFile" : "An-Naziat_42"
    },
    {
        "text" : "فِيمَ أَنْتَ مِنْ ذِكْرَاهَا",
        "translationKey" : "An-Naziat_43_Translation",
        "transliterationKey" : "An-Naziat_43_Transliteration",
        "audioFile" : "An-Naziat_43"
    },
    {
        "text" : "إِلَىٰ رَبِّكَ مُنْتَهَاهَا",
        "translationKey" : "An-Naziat_44_Translation",
        "transliterationKey" : "An-Naziat_44_Transliteration",
        "audioFile" : "An-Naziat_44"
    },
    {
        "text" : "إِنَّمَا أَنْتَ مُنْذِرُ مَنْ يَخْشَاهَا",
        "translationKey" : "An-Naziat_45_Translation",
        "transliterationKey" : "An-Naziat_45_Transliteration",
        "audioFile" : "An-Naziat_45"
    },
    {
        "text" : "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا",
        "translationKey" : "An-Naziat_46_Translation",
        "transliterationKey" : "An-Naziat_46_Transliteration",
        "audioFile" : "An-Naziat_46"
    }
]


