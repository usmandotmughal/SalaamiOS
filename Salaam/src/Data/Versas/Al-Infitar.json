[
    {
        "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
        "translationKey" : "Al-Infitar_0_Translation",
        "transliterationKey" : "Al-Infitar_0_Transliteration",
        "audioFile" : "Al-Infitar_0"
    },
    {
        "text" : "‎إِذَا السَّمَاءُ انْفَطَرَتْ",
        "translationKey" : "Al-Infitar_1_Translation",
        "transliterationKey" : "Al-Infitar_1_Transliteration",
        "audioFile" : "Al-Infitar_1"
    },
    {
        "text" : "‎وَإِذَا الْكَوَاكِبُ انْتَثَرَتْ",
        "translationKey" : "Al-Infitar_2_Translation",
        "transliterationKey" : "Al-Infitar_2_Transliteration",
        "audioFile" : "Al-Infitar_2"
    },
    {
        "text" : "‎وَإِذَا الْبِحَارُ فُجِّرَتْ",
        "translationKey" : "Al-Infitar_3_Translation",
        "transliterationKey" : "Al-Infitar_3_Transliteration",
        "audioFile" : "Al-Infitar_3"
    },
    {
        "text" : "‎وَإِذَا الْقُبُورُ بُعْثِرَتْ",
        "translationKey" : "Al-Infitar_4_Translation",
        "transliterationKey" : "Al-Infitar_4_Transliteration",
        "audioFile" : "Al-Infitar_4"
    },
    {
        "text" : "‎عَلِمَتْ نَفْسٌ مَا قَدَّمَتْ وَأَخَّرَتْ",
        "translationKey" : "Al-Infitar_5_Translation",
        "transliterationKey" : "Al-Infitar_5_Transliteration",
        "audioFile" : "Al-Infitar_5"
    },
    {
        "text" : "‎يَا أَيُّهَا الْإِنْسَانُ مَا غَرَّكَ بِرَبِّكَ الْكَرِيمِ",
        "translationKey" : "Al-Infitar_6_Translation",
        "transliterationKey" : "Al-Infitar_6_Transliteration",
        "audioFile" : "Al-Infitar_6"
    },
    {
        "text" : "‎الَّذِي خَلَقَكَ فَسَوَّاكَ فَعَدَلَكَ",
        "translationKey" : "Al-Infitar_7_Translation",
        "transliterationKey" : "Al-Infitar_7_Transliteration",
        "audioFile" : "Al-Infitar_7"
    },
    {
        "text" : "‎فِي أَيِّ صُورَةٍ مَا شَاءَ رَكَّبَكَ",
        "translationKey" : "Al-Infitar_8_Translation",
        "transliterationKey" : "Al-Infitar_8_Transliteration",
        "audioFile" : "Al-Infitar_8"
    },
    {
        "text" : "‎كَلَّا بَلْ تُكَذِّبُونَ بِالدِّينِ",
        "translationKey" : "Al-Infitar_9_Translation",
        "transliterationKey" : "Al-Infitar_9_Transliteration",
        "audioFile" : "Al-Infitar_9"
    },
    {
        "text" : "‎وَإِنَّ عَلَيْكُمْ لَحَافِظِينَ",
        "translationKey" : "Al-Infitar_10_Translation",
        "transliterationKey" : "Al-Infitar_10_Transliteration",
        "audioFile" : "Al-Infitar_10"
    },
    {
        "text" : "‎كِرَامًا كَاتِبِينَ",
        "translationKey" : "Al-Infitar_11_Translation",
        "transliterationKey" : "Al-Infitar_11_Transliteration",
        "audioFile" : "Al-Infitar_11"
    },
    {
        "text" : "‎يَعْلَمُونَ مَا تَفْعَلُونَ",
        "translationKey" : "Al-Infitar_12_Translation",
        "transliterationKey" : "Al-Infitar_12_Transliteration",
        "audioFile" : "Al-Infitar_12"
    },
    {
        "text" : "‎إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ",
        "translationKey" : "Al-Infitar_13_Translation",
        "transliterationKey" : "Al-Infitar_13_Transliteration",
        "audioFile" : "Al-Infitar_13"
    },
    {
        "text" : "‎وَإِنَّ الْفُجَّارَ لَفِي جَحِيمٍ",
        "translationKey" : "Al-Infitar_14_Translation",
        "transliterationKey" : "Al-Infitar_14_Transliteration",
        "audioFile" : "Al-Infitar_14"
    },
    {
        "text" : "‎يَصْلَوْنَهَا يَوْمَ الدِّينِ",
        "translationKey" : "Al-Infitar_15_Translation",
        "transliterationKey" : "Al-Infitar_15_Transliteration",
        "audioFile" : "Al-Infitar_15"
    },
    {
        "text" : "‎وَمَا هُمْ عَنْهَا بِغَائِبِينَ",
        "translationKey" : "Al-Infitar_16_Translation",
        "transliterationKey" : "Al-Infitar_16_Transliteration",
        "audioFile" : "Al-Infitar_16"
    },
    {
        "text" : "‎وَمَا أَدْرَاكَ مَا يَوْمُ الدِّينِ",
        "translationKey" : "Al-Infitar_17_Translation",
        "transliterationKey" : "Al-Infitar_17_Transliteration",
        "audioFile" : "Al-Infitar_17"
    },
    
    {
        "text" : "‎ثُمَّ مَا أَدْرَاكَ مَا يَوْمُ الدِّينِ",
        "translationKey" : "Al-Infitar_18_Translation",
        "transliterationKey" : "Al-Infitar_18_Transliteration",
        "audioFile" : "Al-Infitar_18"
    },
    {
        "text" : "‎يَوْمَ لَا تَمْلِكُ نَفْسٌ لِنَفْسٍ شَيْئًا ۖ وَالْأَمْرُ يَوْمَئِذٍ لِلَّهِ",
        "translationKey" : "Al-Infitar_19_Translation",
        "transliterationKey" : "Al-Infitar_19_Transliteration",
        "audioFile" : "Al-Infitar_19"
    }
]

