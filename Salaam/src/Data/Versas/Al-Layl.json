[
  {
    "audioFile" : "Al-Layl_0",
    "text" : "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "translationKey" : "Al-Layl_0_Translation",
    "transliterationKey" : "Al-Layl_0_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_1",
    "text" : "وَاللَّيْلِ إِذَا يَغْشَىٰ",
    "translationKey" : "Al-Layl_1_Translation",
    "transliterationKey" : "Al-Layl_1_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_2",
    "text" : "وَالنَّهَارِ إِذَا تَجَلَّىٰ",
    "translationKey" : "Al-Layl_2_Translation",
    "transliterationKey" : "Al-Layl_2_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_3",
    "text" : "وَمَا خَلَقَ الذَّكَرَ وَالْأُنْثَىٰ",
    "translationKey" : "Al-Layl_3_Translation",
    "transliterationKey" : "Al-Layl_3_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_4",
    "text" : "إِنَّ سَعْيَكُمْ لَشَتَّىٰ",
    "translationKey" : "Al-Layl_4_Translation",
    "transliterationKey" : "Al-Layl_4_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_5",
    "text" : "فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ",
    "translationKey" : "Al-Layl_5_Translation",
    "transliterationKey" : "Al-Layl_5_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_6",
    "text" : "وَصَدَّقَ بِالْحُسْنَىٰ",
    "translationKey" : "Al-Layl_6_Translation",
    "transliterationKey" : "Al-Layl_6_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_7",
    "text" : "فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ",
    "translationKey" : "Al-Layl_7_Translation",
    "transliterationKey" : "Al-Layl_7_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_8",
    "text" : "وَأَمَّا مَنْ بَخِلَ وَاسْتَغْنَىٰ",
    "translationKey" : "Al-Layl_8_Translation",
    "transliterationKey" : "Al-Layl_8_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_9",
    "text" : "وَكَذَّبَ بِالْحُسْنَىٰ",
    "translationKey" : "Al-Layl_9_Translation",
    "transliterationKey" : "Al-Layl_9_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_10",
    "text" : "فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ",
    "translationKey" : "Al-Layl_10_Translation",
    "transliterationKey" : "Al-Layl_10_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_11",
    "text" : "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ",
    "translationKey" : "Al-Layl_11_Translation",
    "transliterationKey" : "Al-Layl_11_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_12",
    "text" : "إِنَّ عَلَيْنَا لَلْهُدَىٰ",
    "translationKey" : "Al-Layl_12_Translation",
    "transliterationKey" : "Al-Layl_12_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_13",
    "text" : "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ",
    "translationKey" : "Al-Layl_13_Translation",
    "transliterationKey" : "Al-Layl_13_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_14",
    "text" : "فَأَنْذَرْتُكُمْ نَارًا تَلَظَّىٰ",
    "translationKey" : "Al-Layl_14_Translation",
    "transliterationKey" : "Al-Layl_14_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_15",
    "text" : "لَا يَصْلَاهَا إِلَّا الْأَشْقَى",
    "translationKey" : "Al-Layl_15_Translation",
    "transliterationKey" : "Al-Layl_15_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_16",
    "text" : "الَّذِي كَذَّبَ وَتَوَلَّىٰ",
    "translationKey" : "Al-Layl_16_Translation",
    "transliterationKey" : "Al-Layl_16_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_17",
    "text" : "وَسَيُجَنَّبُهَا الْأَتْقَى",
    "translationKey" : "Al-Layl_17_Translation",
    "transliterationKey" : "Al-Layl_17_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_18",
    "text" : "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ",
    "translationKey" : "Al-Layl_18_Translation",
    "transliterationKey" : "Al-Layl_18_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_19",
    "text" : "وَمَا لِأَحَدٍ عِنْدَهُ مِنْ نِعْمَةٍ تُجْزَىٰ",
    "translationKey" : "Al-Layl_19_Translation",
    "transliterationKey" : "Al-Layl_19_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_20",
    "text" : "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ",
    "translationKey" : "Al-Layl_20_Translation",
    "transliterationKey" : "Al-Layl_20_Transliteration"
  },
  {
    "audioFile" : "Al-Layl_21",
    "text" : "وَلَسَوْفَ يَرْضَىٰ",
    "translationKey" : "Al-Layl_21_Translation",
    "transliterationKey" : "Al-Layl_21_Transliteration"
  }
]